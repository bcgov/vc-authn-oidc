{
  "name": "vue-scaffold-frontend",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit --verbose --verbose --forceExit --detectOpenHandles",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "pretest": "npm run lint",
    "clean": "rm -rf coverage dist **/e2e/videos",
    "purge": "rm -rf node_modules",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^1.0.1",
    "axios": "^0.24.0",
    "core-js": "^3.19.2",
    "keycloak-js": "^15.0.2",
    "nprogress": "^0.2.0",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.3.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-eslint": "^10.1.0",
    "eslint": ">= 6.8.0 < 7",
    "eslint-plugin-prettier": ">= 3.4.0 < 4",
    "eslint-plugin-vue": "^8.1.1",
    "eslint-plugin-vuetify": "^1.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.0",
    "sass": ">= 1.32.13 < 1.33.0",
    "sass-loader": ">= 10.2.0 < 11.0.0",
    "vue-cli-plugin-vuetify": "^2.4.4",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
