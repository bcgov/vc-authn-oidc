{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "IdentityServer": {
    "ConnectionStrings": {
      "Database": "Host=localhost;Port=5432;Database=IdentityServer;User ID=identity;"
    },
    "CertificateFilename": "secret.rsa",
    "PublicOrigin": "http://localhost:5000",
    "AccessTokenLifeTimeSeconds": 172800
  }
}
